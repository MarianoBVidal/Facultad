using System.Runtime.CompilerServices;
using System.Security.Cryptography.X509Certificates;

namespace teoria8;

public class Ingresador
{
    public void Ingresar()
    {
        private EventHandler? _lineaVaciaIngresada;
        public event IngresadorEventHandler LineaVaciaIngresada
        {
            add
            {}
            remove
            {}
        }
        public IngresadorEventHandler? NroIngresdo;

        string st = Console.ReadLine() ?? "";
        int valorNum;
        while (st != "fin")
        {
            st = Console.ReadLine() ?? "";
            if (st == "")
            {

            }
            else if (int.TryParse(st, out valorNum))
            {}
        }
    }
}

namespace teoria8;
public class IngresadorEventArgs : EventArgs
{
    public int Valor { get; set; }
}

namespace teoria8;
public delegate void IngresadorEventHandler(object sender, IngresadorEventArgs e);

using teoria8;
Ingresador ingresador = new Ingresador();
Ingresador.LineaVaciaIngresada += (sender, e) => { Console.WriteLine("Se ingresó una línea en blanco"); };
ingresador.NroIngresado += (sender, e) => { Console.WriteLine($"Se ingresó el número {e.Valor}"); };
Ingresador.Ingresar();
